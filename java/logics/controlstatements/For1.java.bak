class For1 {

    public static void main(String[] args) {

        printing(1, 6);
        System.out.println("the next one");

        printing1(1, 6);
        System.out.println("the next one");

        printing2(1, 6);
        System.out.println("the next one");

        printing3(1, 6);
        System.out.println("the next one");

        printing4(1, 6);
        System.out.println("the next one");

        printing5(1, 6);
        System.out.println("the next one");

        printing6(1, 6);
        System.out.println("the next one");

        printing7(1, 6);
        System.out.println("the next one");

        printing8(1, 6);
        System.out.println("the next one");

        printing9(1, 6);
    }

    public static void printing(int start, int end) {
        for (int i = start; i <= end; i++) {
            System.out.println(i);
        }
    }

    public static void printing1(int start, int end) {
        for (int i = end; i >= start; i--) {
            System.out.println(i);
        }
    }

    public static void printing2(int start, int end) {
        for (int i = start; i <= end - 3; i++) {
            System.out.println(i);
        }
    }

    public static void printing3(int start, int end) {
        for (int i = end - 3; i >= start; i--) {
            System.out.println(i);
        }
    }

    public static void printing4(int start, int end) {
        for (int i = end - 2; i <= end; i++) {
            System.out.println(i);
        }
    }

    public static void printing5(int start, int end) {
        for (int i = end; i >= start + 3; i--) {
            System.out.println(i);
        }
    }

    public static void printing6(int start, int end) {
        for (int i = start; i <= end; i++) {
            if (i % 2 == 0) {
                System.out.println(i);
            }
        }
    }

    public static void printing7(int start, int end) {
        for (int i = end; i >= start; i--) {
            if (i % 2 == 0) {
                System.out.println(i);
            }
        }
    }

    public static void printing8(int start, int end) {
        for (int i = start; i <= end; i++) {
            if (i % 2 != 0) {
                System.out.println(i);
            }
        }
    }

    public static void printing9(int start, int end) {
        for (int i = end; i >= start; i--) {
            if (i % 2 != 0) {
                System.out.println(i);
            }
        }
    }
}
