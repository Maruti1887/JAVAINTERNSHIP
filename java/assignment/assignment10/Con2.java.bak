class Con2{
	String c_name;
	int c_cost;
	String c_type;
	Con2(String c_name, int c_cost,String c_type){
	 this.c_name=c_name;
	 this.c_cost=c_cost;
	 this.c_type=c_type;
	 }
	 public static void main(String [] args){
		Con2 C1=new Con2("AUDI", 10000000,"Electric");
		System.out.println(" The car name is " +C1.c_name);
		System.out.println(" The car cost is " +C1.c_cost);
		System.out.println(" the car type is " +C1.c_type);
	 }
}